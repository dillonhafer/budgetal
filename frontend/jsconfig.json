{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src/",
    "paths": {
      "actions/*": ["actions/*"],
      "api/*": ["api/*"],
      "constants/*": ["constants/*"],
      "css/*": ["css/*"],
      "images/*": ["images/*"],
      "layouts/*": ["layouts/*"],
      "reducers/*": ["reducers/*"],
      "routes/*": ["routes/*"],
      "utils/*": ["utils/*"],
      "window": ["utils/window.js"]
    }
  },
  "exclude": ["node_modules"]
}
